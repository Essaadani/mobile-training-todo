<?xml version="1.0" encoding="UTF-8"?>
<TabbedPage
    xmlns:d="clr-namespace:CouchbaseLabs.MVVM.Forms.Pages;assembly=CouchbaseLabs.MVVM.Forms"
    xmlns="http://xamarin.com/schemas/2014/forms" 
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="Training.Views.ListDetailPage"
    Title="{Binding PageTitle}">
    <TabbedPage.ToolbarItems>
        <ToolbarItem Text="Users" Clicked="OnPageSelect_Clicked" />
        <ToolbarItem Text="+" Clicked="OnAdd_Clicked" />
    </TabbedPage.ToolbarItems>
</TabbedPage>
