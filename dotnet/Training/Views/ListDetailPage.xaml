<?xml version="1.0" encoding="UTF-8"?>
<d:BaseTabbedPage
    xmlns:d="clr-namespace:Prototype.Mvvm.Forms.Pages;assembly=Prototype.Mvvm.Forms"
    xmlns:vm="clr-namespace:Training.ViewModels;assembly=Training.Core"
    xmlns="http://xamarin.com/schemas/2014/forms" 
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:TypeArguments="vm:ListDetailViewModel"
    x:Class="Training.Views.ListDetailPage"
    Title="{Binding PageTitle}">
    <d:BaseTabbedPage.ToolbarItems>
        <ToolbarItem Text="Users" Clicked="OnPageSelect_Clicked" />
        <ToolbarItem Text="+" Command="{Binding AddCommand}" />
    </d:BaseTabbedPage.ToolbarItems>
</d:BaseTabbedPage>
